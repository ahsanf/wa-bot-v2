    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/qrcode-terminal": "^0.12.2",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http": "^0.0.1-security",
    "openai": "^4.72.0",
    "puppeteer": "^23.8.0",
    "qrcode-terminal": "^0.12.0",
